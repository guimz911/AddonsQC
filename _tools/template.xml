<?xml version="1.0" encoding="UTF-8"?>
    <addon id="{addonid}" name="AddonQC" version="1.0" provider-name="Guimz">
        <requires>
            <import addon="xbmc.addon" version="1.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="AddonQC">
            <info compressed="false">{url}{output_path}addons.xml</info>
            <checksum>{url}{output_path}addons.xml.md5</checksum>
            <datadir zip="true">{url}{output_path}</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>{summary}</summary>
            <description>Source de addon francophone</description>
            <platform>all</platform>
    </extension>
</addon>